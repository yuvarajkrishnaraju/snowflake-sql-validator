{
  "name": "snowflake-sql-validator",
  "version": "1.0.0",
  "description": "Snowflake SQL validator for React applications",
  "license": "ISC",
  "author": "",
  "type": "commonjs",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "npm run build",
    "prettify": "prettier --ignore-path \"./.prettierignore\" --write \"./**/*.{js,jsx,json,ts,tsx}\" --config ./.prettierrc",
    "publish:public": "npm publish --access public"
  },
  "dependencies": {
    "antlr4ts": "^0.5.0-alpha.4"
  },
  "devDependencies": {
    "@types/jest": "^29.0.0",
    "@types/node": "^20.0.0",
    "assert": "^2.1.0",
    "jest": "^29.0.0",
    "prettier": "^3.6.2",
    "process": "^0.11.10",
    "ts-jest": "^29.0.0",
    "typescript": "^5.9.2",
    "util": "^0.12.5"
  },
  "keywords": [
    "snowflake",
    "sql",
    "validator",
    "parser",
    "antlr4",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": ""
  }
}
