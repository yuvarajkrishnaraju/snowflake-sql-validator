{
  "name": "snowflake-sql-validator",
  "version": "1.1.3",
  "description": "Snowflake SQL validator for React applications",
  "license": "ISC",
  "author": "",
  "type": "commonjs",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc --watch",
    "clean": "rm -rf dist",
    "prebuild": "npm run clean",
    "test": "jest",
    "test:watch": "jest --watch",
    "prepare": "npm run build",
    "prettify": "prettier --ignore-path \"./.prettierignore\" --write \"./**/*.{js,jsx,json,ts,tsx}\" --config ./.prettierrc",
    "publish:public": "npm publish --access public",
    "version:patch": "node scripts/version-bump.js patch",
    "version:minor": "node scripts/version-bump.js minor",
    "version:major": "node scripts/version-bump.js major",
    "release:patch": "npm run version:patch && npm run publish:public",
    "release:minor": "npm run version:minor && npm run publish:public",
    "release:major": "npm run version:major && npm run publish:public",
    "release:preview": "npm run build && npm publish --tag beta --access public",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "changelog:first": "conventional-changelog -p angular -i CHANGELOG.md -s -r 0",
    "prepublishOnly": "npm run build",
    "postversion": "git push && git push --tags",
    "lint": "eslint src/**/*.ts --ext .ts",
    "lint:fix": "eslint src/**/*.ts --ext .ts --fix",
    "type-check": "tsc --noEmit",
    "check-all": "npm run lint && npm run type-check && npm run test",
    "pre-commit": "npm run check-all",
    "format:staged": "lint-staged",
    "format:check": "prettier --check \"./**/*.{js,jsx,json,ts,tsx}\""
  },
  "dependencies": {
    "antlr4ts": "^0.5.0-alpha.4"
  },
  "peerDependencies": {
    "process": "^0.11.10",
    "util": "^0.12.5",
    "assert": "^2.1.0"
  },
  "peerDependenciesMeta": {
    "process": {
      "optional": false
    },
    "util": {
      "optional": false
    },
    "assert": {
      "optional": false
    }
  },
  "devDependencies": {
    "@types/jest": "^30.0.0",
    "@types/node": "^24.3.0",
    "assert": "^2.1.0",
    "jest": "^30.1.1",
    "prettier": "^3.6.2",
    "process": "^0.11.10",
    "ts-jest": "^29.4.1",
    "typescript": "^5.9.2",
    "util": "^0.12.5",
    "conventional-changelog-cli": "^5.0.0",
    "eslint": "^9.34.0",
    "@typescript-eslint/eslint-plugin": "^8.41.0",
    "@typescript-eslint/parser": "^8.41.0",
    "husky": "^9.1.7",
    "lint-staged": "^16.1.5"
  },
  "keywords": [
    "snowflake",
    "sql",
    "validator",
    "parser",
    "antlr4",
    "typescript",
    "react",
    "node"
  ],
  "repository": {
    "type": "git",
    "url": ""
  },
  "engines": {
    "node": ">=14.0.0"
  }
}
